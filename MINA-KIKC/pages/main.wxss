}

/* 九宫格菜单 */

.menu {
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  overflow: hidden;
  background: #fff;
}

.menu__item {
  width: 33.33333%;
  position: relative;
}

.menu .menu__item__inner {
  min-height: 70rpx;
  text-align: center;
  border-right: 1px solid #fff;
  border-bottom: 1px solid #fff;
  margin-right: 4px;
  margin-bottom: 4px;
  padding-top: 60rpx;
  padding-bottom: 60rpx;
  display: block;
  border-radius: 0;
  background-color: #fff;
  font-size: 13px;
}

.menu__item:nth-child(3n+3) .menu__item__inner {
  /*border-right: 0;*/
}

.menu__item:nth-child(3n+1) .menu__item__inner {
  border-left: 1px solid #fff;
  margin-left: 4px;
}

.menu__item__left:nth-child(3n+1) .menu__item__inner {
  border-left: 1px solid #d9d9d9;
}

.menu__item:nth-child(1) .menu__item__inner {
  border-top: 1px solid #fff;
  margin-top: 4px;
}

.menu__item:nth-child(2) .menu__item__inner {
  border-top: 1px solid #fff;
  margin-top: 4px;
}

.menu__item:nth-child(3) .menu__item__inner {
  border-top: 1px solid #fff;
  margin-top: 4px;
}

.menu__icon {
  width: 60rpx;
  height: 60rpx;
}

button.menu__item__inner {
  min-height: 70rpx;
  border: none;
  border-right: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
  margin-right: 4px;
  margin-bottom: 4px;
  background-color: transparent;
  padding-top: 60rpx;
  padding-bottom: 60rpx;
  font-size: 13px;
  margin: 0;
  line-height: inherit;
}

button.menu__item__inner::after {
  display: none;
}

.menu__item__inner {
  position: relative;
}

/* .menu__item .cell {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
} */

.menu__item .concell {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 1;
}

.concell cell {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.concell .cell--row-hover-class, .concell .cell--row {
  width: 100%;
  height: 111px;
}

.cell--bubble {
  width: 100%;
  height: 100%;
}

.num_icon {
  position: absolute;
  left: 50%;
  margin-left: 10px;
  width: 20px;
  height: 20px;
  background: #bd9659;
  color: #fff;
  border-radius: 50%;
  line-height: 20px;
  text-align: center;
  box-shadow: 0 0 1px #ddd;
}

