.navigator-arrow {
  padding-right: 16rpx;
  position: relative;
}

.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #c8c8cb;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -12rpx;
  right: 10rpx;
}

.linear {
  width: 100%;
}

.linear-item {
  width: 100%;
  position: relative;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 28rpx;
}

.linear__item__inner {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin: 0;
}

.linear__item__inner:before {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
  left: 15px;
}

.linear__item__inner .linear__num__icon {
  position: absolute;
  right: 30rpx;
  margin-left: 10px;
  width: 40rpx;
  height: 40rpx;
  background: #d10504;
  color: #fff;
  border-radius: 50%;
  line-height: 40rpx;
  text-align: center;
  /* box-shadow:0 0 1px #ddd; */
}

.linear__item__inner .linear__icon {
  width: 48rpx;
  height: 48rpx;
}

.linear__item__inner .linear__name {
  font-size: 28rpx;
  flex: 1;
  margin-left: 20rpx;
}

