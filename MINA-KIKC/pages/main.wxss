/*立即领卡*/

.getvipcard {
  position: absolute;
  bottom: 7px;
  right: 20px;
  display: inline-block;
  line-height: 15px;
  height: 15px;
  padding: 2px;
  text-align: center;
  background-color: transparent;
  border: 1px solid #fff;
  color: #fff;
  border-radius: 5px;
}

.dialog {
  background: rgba(0, 0, 0, 0.6);
  width: 100%;
  position: fixed;
  box-sizing: border-box;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 500;
}

.dialogWrap {
  width: 100%;
  height: 100%;
  position: relative;
}

.dialogCtn {
  position: absolute;
  width: 500rpx;
  background: #fff;
  height: 290rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.submitbtn-wrap {
  text-align: center;
  padding: 40rpx 0;
}

.submitbtn-a {
  display: inline-block;
  width: 225px;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #f60;
  color: #fff;
  font-size: 16px;
}

.dialogClose {
  width: 30px;
  height: 30px;
}

/* 注册面板 */

.userreg {
