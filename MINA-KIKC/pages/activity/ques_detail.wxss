.vote_form .section_tit{
	font-weight: bold;
}

.vote_form label.radio{
	display: block;
	margin-bottom: 10px;
	overflow: hidden;
}

.vote_form label.checkbox{
	display: block;
	margin-bottom: 10px;
}

.vote_form .vote_ques_col2  label.checkbox{
	width:50%;
	display: inline-block;
}
.vote_form .vote_ques_col3  label.checkbox{
	width:33%;
	margin-right:0.33%;
	display: inline-block;
}
.vote_form .vote_ques_col4  label.checkbox{
	width:24.7%;
	margin-right:0.247%;
	display: inline-block;
}
.vote_form .vote_ques_col5  label.checkbox{
	width:19.7%;
	margin-right:0.197%;
	display: inline-block;
}
.vote_form .vote_ques_col2  label.radio{
	width:50%;
