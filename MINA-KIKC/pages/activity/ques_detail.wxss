}
.vote_form .vote_ques_col2  label.img_radio{
	width:50%;
	display: inline-block;
}
.vote_form .vote_ques_col3  label.img_radio{
	width:33%;
	margin-right:0.33%;
	display: inline-block;
}

.vote_form .vote_ques_col4  label.img_radio{
	width:24.7%;
