.prodgroup_size_T .box{width:100%;display: inline-block;}

/* L图 */
.prodgroup_size_L .prod_cover{padding-bottom: 35%;}
.prodgroup_size_L .box-list{width:100%; background: #fff; margin-bottom: 10px;}
.prodgroup_size_L .box-list .prod_cover{float: left; width: 30%;}
.prodgroup_size_L .box-list .disp_prod_name{float: left; width: 65%; padding-left: 20rpx;}
.prodgroup_size_L .box-list .disp_prod_price{float: left; width: 65%; padding-left: 20rpx;}
.prodgroup_size_L .box-list .disp_orig_price{padding-left: 20rpx; float: left;}
.prodgroup_size_L .disp_buy_btn{position: absolute; bottom:20rpx; right: 0;}
.prodgroup_size_L .disp_collet_btn{position: absolute; top:20rpx; right: 0; z-index: 100;}
.prodgroup_size_L image{width:100%; height: 150px;}

