page{
	overflow-x: hidden;
  height: 100%;
}
/* 广告 */
.pagebg{
  padding-bottom: 81px;
}
.control_bannar image{
	width: 100%;
}
.bannar_size_S .bannar_item{
	width: 50%;
	display: inline-block;
	height: 50%;
	overflow: hidden;
}
.bannar_size_S .bannar_item image{
  width: 100%;
  height: 86px;
}
.slide-image{
  height: 350rpx;
  float: left;

}
.prod_list{
  float: left;
  margin-left: 10px;
}

/* 选择商品 */
.control_prodtype{padding: 4px;}
.control_prodclass{padding: 4px;}
.control_shop{padding: 4px;}
.control_group{padding:4px;}

.prodtype_inner .prod_space{width:50%;}
.prod_space .disp_prod_name{
    display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;
    overflow: hidden;height: 32px;line-height: 16px;padding-top: 1px; font-size: 12px; margin-left: 5rpx;
}

.prod_space .disp_price{color: #c40;line-height: 30px; float: left; width: 65%;}
.prod_space .disp_orig_price{line-height: 20px;display:block;font-size: 12px;width:120px; margin-left: 5rpx;}
.prodtype_size_L .prod_space .disp_orig_price{
  float: left;
}
.prodtype_size_L .prod_space .disp_price{padding-left: 15rpx;}
.prod_space .disp_saleqty{
    float: left;
    font-size: 12px;
    line-height: 30px;
    width: 35%;
}
.prod_cover{
  width:100%;
  height:0;
  padding-bottom:100%;
  position:relative;
  display: block;
}
.disp_collet_btn{
  position: absolute; 
  top: 10rpx; 
  right: 10rpx; 
  padding: 10rpx;
  background: #ddd;
  border-radius:  50%;
	z-index: 2;
  height: 40rpx;
}
.disp_buy_btn{
  position: absolute; 
  bottom: 50rpx; 
  right: 10rpx; 
  padding: 10rpx;
  background: #ddd;
  border-radius:  50%;
  height: 40rpx;
  z-index: 100;
}
.control_prodtype .prod_cover image{width:100%;display: block;}
.control_prodclass .prod_cover image{width:100%;display: block;}
.control_prodclass .prod_cover{width:100%;height:0;padding-bottom:100%;position:relative;display: block;}
.control_shop .prod_cover image{width:100%;}
.control_shop .prod_cover{width:100%;height:0;padding-bottom:100%;position:relative;}
.imghidden{
  overflow:  hidden;
}
.prod_spaces{
  background: #fff;
  text-align: center;
  font-size: 15px;
  padding: 10rpx 0rpx;
  margin-top: 15px;
}

.prod_space_inner{position: relative;background: #fff;margin-bottom: 10px;}
/* B图 */
.prodtype_size_B .prod_space{width:100%;}
/* S图 */
.prodtype_size_S .prod_space{width:50%;display: inline-block;}
.prodtype_size_S .prod_space:nth-child(2n+1) .prod_space_inner{margin-right: 2px;}
.prodtype_size_S .prod_space:nth-child(2n) .prod_space_inner{margin-left: 2px;}
/* T图 */
.prodtype_size_T .prod_space:nth-child(3n+1){width:100%;}
.prodtype_size_T .prod_space{width:50%;display: inline-block;}
.prodtype_size_T .prod_space:nth-child(3n+2) .prod_space_inner {margin-right: 2px;}
.prodtype_size_T .prod_space:nth-child(3n+3) .prod_space_inner {margin-left: 2px;}
/* L图 */
.prodtype_size_L .prod_space{width:100%;}
.prodtype_size_L .prod_space .prod_cover{float: left; width: 30%; padding-bottom: 30%;}
.prodtype_size_L .prod_space .disp_prod_name{float: left; width: 65%; padding-left: 20rpx;}
.prodtype_size_L .prod_space .disp_prod_price{float: left; width: 65%; padding-left: 20rpx;}
.prodtype_size_L .prod_space .disp_orig_price{padding-left: 20rpx;}
.prodtype_size_L .disp_buy_btn{position: absolute; bottom:20rpx; right: 0; z-index: 100;}
.prodtype_size_L .disp_collet_btn{position: absolute; top:20rpx; right: 0;}
/* 店铺 */
.prodtype_size_1 .prod_space{width:100%;}
.prodtype_size_1 .prod_space image{width:100%; height:340px;}
.prodtype_size_2 .prod_space{width:50%;display: inline-block;}
.prodtype_size_2 .prod_space image{width:100%; height:181px;}

/* 搜索 */
.search_inner input{
	display: block;
	background: #fff;
	border-radius: 100px;
	padding: 3px 0;
	text-align: center;
	font-size:13px;
}
.search_inner{
 position: relative;
}
.search_img{
  width: 20px;
  height: 20px;
  position: absolute;
  right: 18px;
  top: 16px;
  z-index: 10;
}
/* 文字标题 */
.textTitle_inner{
	padding: 4px;
	background-color: #fff;
}
.textTitle_innerC{
	text-align: center
}
.textTitle_innerR{
	text-align: right
}
.textTitle_tit{
	margin-bottom: 5px;
  font-weight:  bold;
  font-size: 18px;
}
.textTitle_sub{
  font-size: 14px;
}

/* 优惠券 */
.coupon_inner{
	padding:4px;
	text-align: center;
}
.coupon_inner .coupon_space{
	width: 33%;
  display: inline-block;
	margin-right: 0.3%;
}

.coupon_inner .coupon_space .coupon_space_inner{
    padding: 10px 10px;
    background: #fdc6d0;
    text-align: center;
    color: #e82347;
    border: 1px dashed #e82347;
    border-radius: 6px;
    overflow: hidden;
}
.coupon_inner .coupon_space .coupon_tit{
	display: block;
	font-weight: bold;
	line-height: 20px;
  height: 20px;
  overflow: hidden;
  font-size: 14px;
}
.coupon_inner .coupon_space .coupon_sub{
	font-size: 12px;
}

/* 公告 */
.control_notice{width: 100%;overflow: hidden;position: relative;}
.notice_inner{
	  padding: 10px;
    background: #ffe0b4;
    color: #f99601;
}

/* 商品分组 */
.prod_group{ 
  display: none;
}  
.curt{
  display: block;
}

.clear{
  clear: both;
}

.clearfix:after{
  content: '';
  display: block;
  clear: both;
}

